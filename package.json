{
  "name": "transcripts",
  "productName": "transcripts",
  "version": "1.0.1",
  "description": "A simple Electron app to fetch YouTube transcripts",
  "main": "src/index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": {
    "name": "Zyztem",
    "email": "me@zyztem.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zyztem/youtube-transcript-fetcher.git"
  },
  "license": "MIT",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "youtube-transcript": "^1.2.1"
  },
  "devDependencies": {
    "electron-builder": "^26.0.12",
    "electron": "^35.7.5",
    "dmg-license": "^1.0.3"
  },
  "build": {
    "appId": "dev.zyztem.youtube-transcript-fetcher",
    "productName": "Youtube Transcript Fetcher",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        {
          "target": "msi",
          "arch": ["x64"]
        }
      ]
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": ["x64", "arm64"],
          "artifactName": "Youtube Transcript Fetcher-${version}-${arch}.dmg"
        }
      ],
      "category": "public.app-category.utilities",
      "minimumSystemVersion": "10.10"
    },
    "linux": {
      "target": [
        {
          "target": "deb",
          "arch": ["x64", "arm64"]
        }
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "zyztem",
        "repo": "youtube-transcript-fetcher"
      }
    ]
  },
  "packageManager": "pnpm@10.7.0+sha512.6b865ad4b62a1d9842b61d674a393903b871d9244954f652b8842c2b553c72176b278f64c463e52d40fff8aba385c235c8c9ecf5cc7de4fd78b8bb6d49633ab6"
}
